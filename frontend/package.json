{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "STATIC_URL=http://localhost:3000/ vue-cli-service serve",
    "build": "STATIC_URL=/static/ vue-cli-service build",
    "lint": "vue-cli-service lint",
    "spec": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "testPathIgnorePatterns": ["<rootDir>/src/components/__tests__/helpers"],
    "moduleNameMapper": {
      "^/components/([^\\.]*)$": "<rootDir>/src/components/$1.vue",
      "^@[/](.+)": "<rootDir>/src/$1",
      "^src/components/([^\\.]*)$": "<rootDir>/src/components/$1.vue"
    },
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "customExportConditions": [
        "node",
        "node-addons"
      ]
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "@vue/vue3-jest"
    }
  },
  "dependencies": {
    "@vue/compiler-sfc": "3.2.39",
    "@vue/test-utils": "^2.0.2",
    "@vue/vue3-jest": "28",
    "@vuepic/vue-datepicker": "^3.5.2",
    "axios": "^0.27.2",
    "babel-jest": "^29.0.3",
    "core-js": "^3.6.5",
    "dayjs": "^1.11.5",
    "eslint-plugin-no-comments": "^1.0.5",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "tw-elements": "^1.0.0-alpha12",
    "vue": "^3.2.39",
    "vue-router": "4",
    "vue3-google-login": "^2.0.12",
    "vuex": "^4.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "autoprefixer": "^9",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "postcss": "^7",
    "sass": "^1.32.7",
    "sass-loader": "9.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "webpack4-bundle-tracker": "^0.0.1-beta"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
